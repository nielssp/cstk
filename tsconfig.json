{
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "es5",
    "lib": ["es2015", "dom"],
    "outDir": "lib",
    "strict": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "jsx": "react",
    "jsxFactory": "elem"
  }
}
